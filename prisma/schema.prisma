datasource db {
  provider = "sqlite" // or "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Subscription {
  id         String   @id @default(cuid())
  endpoint   String   @unique
  keysAuth   String
  keysP256dh String
  createdAt  DateTime @default(now())
}
